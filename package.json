{
    "private": true,
    "packageManager": "pnpm@10.16.0",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "node scripts/dev.js",
        "build": "pnpm --filter starlex/core build",
        "format": "prettier --write --cache --parser typescript \"packages/**/*.ts?(x)\"",
        "format-check": "prettier --check --cache --parser typescript \"packages/**/*.ts?(x)\"",
        "test": "pnpm --filter starlex/core test",
        "release": "node scripts/release.js"
    },
    "types": "test-dts/index.d.ts",
    "devDependencies": {
        "@types/eslint": "catalog:",
        "@types/node": "catalog:",
        "@typescript-eslint/eslint-plugin": "catalog:",
        "@typescript-eslint/parser": "catalog:",
        "dotenv": "catalog:",
        "eslint": "catalog:",
        "eslint-config-prettier": "catalog:",
        "prettier": "catalog:",
        "prettier-plugin-multiline-arrays": "catalog:",
        "tsup": "catalog:",
        "typescript": "catalog:",
        "typescript-eslint": "catalog:",
        "vite": "catalog:",
        "vite-plugin-dts": "catalog:",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
