{
    "name": "@cyysummer/core",
    "version": "0.0.9",
    "license": "MIT",
    "type": "module",
    "module": "./dist/index.js",
    "types": "./types",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./types/index.d.ts"
        }
    },
    "scripts": {
        "dev": "tsup --watch",
        "clean": "rm -rf ./dist",
        "prebuild": "tsup --dts-only",
        "build": "tsup --no-dts",
        "postbuild": "mv -f dist/index.d.ts types/lib.d.ts",
        "release": "pnpm run clean && pnpm run build",
        "test": "vitest"
    },
    "dependencies": {
    },
    "peerDependencies": {
        "chalk": ">=5",
        "mitt": ">=3"
    },
    "devDependencies": {
        "tsup": "catalog:",
        "typescript": "catalog:",
        "vite": "catalog:",
        "vitest": "catalog:"
    },
    "publishConfig": {
        "access": "public"
    }
}
